{
  "rabbit_version": "3.12.0",
  "rabbitmq_version": "3.12.0",
  "product_name": "RabbitMQ",
  "product_version": "3.12.0",
  "users": [
    {
      "name": "ecommerce",
      "password_hash": "changeme_in_production",
      "hashing_algorithm": "rabbit_password_hashing_sha256",
      "tags": "administrator"
    }
  ],
  "vhosts": [
    {
      "name": "/"
    },
    {
      "name": "ecommerce"
    }
  ],
  "permissions": [
    {
      "user": "ecommerce",
      "vhost": "/",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    },
    {
      "user": "ecommerce",
      "vhost": "ecommerce",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    }
  ],
  "topic_permissions": [],
  "parameters": [],
  "global_parameters": [
    {
      "name": "cluster_name",
      "value": "ecommerce-cluster"
    }
  ],
  "policies": [
    {
      "vhost": "ecommerce",
      "name": "ha-all",
      "pattern": ".*",
      "apply-to": "all",
      "definition": {
        "ha-mode": "all",
        "ha-sync-mode": "automatic"
      },
      "priority": 0
    }
  ],
  "queues": [
    {
      "name": "email-notifications",
      "vhost": "ecommerce",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 3600000,
        "x-max-length": 10000,
        "x-dead-letter-exchange": "dlx",
        "x-dead-letter-routing-key": "email-notifications.failed"
      }
    },
    {
      "name": "sms-notifications",
      "vhost": "ecommerce",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 3600000,
        "x-max-length": 10000,
        "x-dead-letter-exchange": "dlx",
        "x-dead-letter-routing-key": "sms-notifications.failed"
      }
    },
    {
      "name": "push-notifications",
      "vhost": "ecommerce",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 3600000,
        "x-max-length": 10000
      }
    },
    {
      "name": "order-confirmation-emails",
      "vhost": "ecommerce",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 3600000,
        "x-priority": 10
      }
    },
    {
      "name": "shipping-notifications",
      "vhost": "ecommerce",
      "durable": true,
      "auto_delete": false
    },
    {
      "name": "inventory-sync",
      "vhost": "ecommerce",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-max-priority": 5
      }
    },
    {
      "name": "analytics-processing",
      "vhost": "ecommerce",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 86400000
      }
    },
    {
      "name": "report-generation",
      "vhost": "ecommerce",
      "durable": true,
      "auto_delete": false
    }
  ],
  "exchanges": [
    {
      "name": "notifications",
      "vhost": "ecommerce",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "orders",
      "vhost": "ecommerce",
      "type": "fanout",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "analytics",
      "vhost": "ecommerce",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "dlx",
      "vhost": "ecommerce",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    }
  ],
  "bindings": [
    {
      "source": "notifications",
      "vhost": "ecommerce",
      "destination": "email-notifications",
      "destination_type": "queue",
      "routing_key": "email.*",
      "arguments": {}
    },
    {
      "source": "notifications",
      "vhost": "ecommerce",
      "destination": "sms-notifications",
      "destination_type": "queue",
      "routing_key": "sms.*",
      "arguments": {}
    },
    {
      "source": "notifications",
      "vhost": "ecommerce",
      "destination": "push-notifications",
      "destination_type": "queue",
      "routing_key": "push.*",
      "arguments": {}
    },
    {
      "source": "notifications",
      "vhost": "ecommerce",
      "destination": "order-confirmation-emails",
      "destination_type": "queue",
      "routing_key": "email.order.confirmation",
      "arguments": {}
    },
    {
      "source": "notifications",
      "vhost": "ecommerce",
      "destination": "shipping-notifications",
      "destination_type": "queue",
      "routing_key": "email.shipping.*",
      "arguments": {}
    },
    {
      "source": "analytics",
      "vhost": "ecommerce",
      "destination": "analytics-processing",
      "destination_type": "queue",
      "routing_key": "#",
      "arguments": {}
    },
    {
      "source": "dlx",
      "vhost": "ecommerce",
      "destination": "failed-messages",
      "destination_type": "queue",
      "routing_key": "#",
      "arguments": {}
    }
  ]
}
